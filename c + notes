String nomeMaiuscula = "Dario";
        String nomeMinuscula = "dario";
        String mostrar = "";

        int tam = nomeMinuscula.length();
        for (int i = 0; i < tam; i++) {

            if (nomeMinuscula.charAt(i) == 'd') {
                nomeMinuscula.replace(nomeMinuscula.charAt(i),
                        nomeMaiuscula.charAt(i));
            }
        }

        nomeMinuscula = mostrar;
        System.out.println("" + nomeMinuscula);
 